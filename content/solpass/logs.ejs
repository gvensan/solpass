<script>
  var refreshLogsButton = document.getElementById('refreshLogsButton');
  refreshLogsButton.addEventListener('click', function() {
    window.location.reload();
  });

  var emptyLogsButton = document.getElementById('emptyLogsButton');
  emptyLogsButton.addEventListener('click', function() {
    fetch('/logs/empty', {
      method: 'POST'
    }).then(function(response) {
      if (response.ok) {
        window.location.reload();
      }
    });
  });
</script>